<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>DirectForge@8pecxstudios.com</em:id>
    <em:version>1.0</em:version>
	<em:type>2</em:type>
	<em:iconURL>chrome://DirectForge/skin/icon32.png</em:iconURL>
	<em:icon64URL>chrome://DirectForge/skin/icon64.png</em:icon64URL>
    <!-- Target Application this theme can install into, 
        with minimum and maximum supported versions. --> 
		
    <em:targetApplication>
      <Description>
		<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>29.0</em:minVersion>
        <em:maxVersion>42.*</em:maxVersion>
      </Description>
    </em:targetApplication>
	
	<!-- Front End MetaData -->
    <em:name>DirectForge</em:name>
    <em:description>When enabled changes firefox user-agent to google chrome's to allow direct download on sourceforge on installer enabled projects (No web installer).</em:description>
    <em:creator>8pecxstudios</em:creator>
    <em:homepageURL>https://8pecxstudios.com/</em:homepageURL>
  </Description>      
</RDF>